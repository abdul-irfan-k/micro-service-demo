{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@events/*":["src/events/*"],
      "@lib/*":["src/lib/*"],
      "@route/*":["src/route/*"],
      "@/*":["src/*"],

    },
    "outDir": "./dist",
  },
  "include": ["src/**/*"]
}
{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@config/*": [
        "./src/config/*"
      ],
      "@events/*": [
        "./src/events/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@route/*": [
        "./src/route/*"
      ],
      
    }
  }
}